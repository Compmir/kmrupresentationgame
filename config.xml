 <plugin name="cocoon-plugin-notifications-android-local" spec="*"/>

		<platform name="android">

			<preference name="enabled" value="true"/>

		</platform>

		<plugin name="cordova-plugin-websocket" spec="*"/>



<preference name="Fullscreen" value="true"/>

